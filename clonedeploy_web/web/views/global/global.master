       <div class="sub-nav-top">
        <div id="Level1" runat="server">
        <ul>
           
            
             <li id="subnav-partition">
                <a href="<%= ResolveUrl("~/views/global/partitions/search.aspx?cat=sub1") %>" class="icon-host">
                    <span class="nav-text">Partition Layouts</span>
                </a>
            </li>
            
             <li id="scripts">
                <a href="<%= ResolveUrl("~/views/global/scripts/search.aspx?cat=sub1") %>" class="icon-host">
