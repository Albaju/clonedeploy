                <a href="<%= ResolveUrl("~/views/admin/settings.aspx") %>" class="icon-host">
                    <span class="nav-text">Buildings</span>
                </a>
            </li>
            
             <li id="subnav-server">
                <a href="<%= ResolveUrl("~/views/admin/settings.aspx") %>" class="icon-host">
